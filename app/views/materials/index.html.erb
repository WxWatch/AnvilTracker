<p id="notice"><%= notice %></p>
<h1>Listing Materials</h1>
<div class="index_tables">
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Manufacturer</th>
        <th>Description</th>
        <th>Extruder temp</th>
        <th>Bed temp</th>
        <th>Notes</th>
        <th>Density</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <% @materials.each do |material| %>
      <tr>
        <td><%= material.name %></td>
        <td><%= material.manufacturer %></td>
        <td><%= material.description %></td>
        <td><%= material.extruder_temp %></td>
        <td><%= material.bed_temp %></td>
        <td><%= material.notes %></td>
        <td><%= material.density %></td>
        <td>
          <%= link_to material_path(material) do %>
            <span class="glyphicon glyphicon-info-sign"></span>
          <% end %>
          <%= link_to edit_material_path(material) do %>
            <span class="glyphicon glyphicon-pencil"></span>
          <% end %>
          <%= link_to material_path(material),
                method: :delete,
                data: { confirm: 'Are you sure?' } do %>
           <span class="glyphicon glyphicon-trash"></span>
          <%end %>
        </td>
      </tr>
    <% end %>
  </table>
</div>
<p>
  <br><%= link_to 'New Material', new_material_path %>
</p>